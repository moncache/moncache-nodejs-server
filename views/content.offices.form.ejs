<h1>OFFICES / FORM</h1>

 <form role="form">
  <div class="form-group">
    <label for="name">Name:</label>
    <input id="name" ng-model="office.name" type="text" class="form-control">
  </div>
  <div class="form-group">
    <label for="address">Address:</label>
    <input id="address" ng-model="office.address" type="text" class="form-control">
  </div>
  <input type="hidden" ng-model="office.id" ng-value="office.id">
  <button ng-click="submitForm(office)" type="submit" class="btn btn-default">Ok</button>
</form>
